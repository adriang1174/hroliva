<!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN" "http://www.w3.org/TR/html4/loose.dtd">
<html>
<head>
<meta http-equiv="content-type" content="text/html; charset=windows-1252">
<title>gallery2</title>
<meta content="CodeCharge Studio 4.00.00.04" name="GENERATOR">
<link href="Styles/Simple/Style_doctype.css" type="text/css" rel="stylesheet">
</head>
<body>
<div align="center">
  <!-- BEGIN Grid fotos -->
  <table cellspacing="0" cellpadding="0" border="0">
    <tr>
      <td valign="top">
        <table class="Header" cellspacing="0" cellpadding="0" border="0">
          <tr>
            <td class="HeaderLeft"><img alt="" src="Styles/Simple/Images/Spacer.gif" border="0"></td> 
            <td class="th"><strong>Galeria de Fotos</strong></td> 
            <td class="HeaderRight"><img alt="" src="Styles/Simple/Images/Spacer.gif" border="0"></td>
          </tr>
        </table>
 
        <table class="Grid" cellspacing="0" cellpadding="0">
          <!-- BEGIN Row -->
          <!-- BEGIN Panel RowOpenTag -->
          <tr class="Row">
            <!-- END Panel RowOpenTag -->
            <td>
              <!-- BEGIN Panel RowComponents --><img id="fotosRowComponentsurl_{fotos:rowNumber}" alt="" src="images/{url}" width="300"><!-- END Panel RowComponents --></td> 
            <!-- BEGIN Panel RowCloseTag -->
          </tr>
 <!-- END Panel RowCloseTag --><!-- END Row -->
          <!-- BEGIN NoRecords -->
          <tr class="NoRecords">
            <td colspan="3">No hay registros</td>
          </tr>
          <!-- END NoRecords -->
          <tr class="Footer">
            <td colspan="3">
              <!-- BEGIN Navigator Navigator -->
              <!-- BEGIN Prev_On --><a href="{Prev_URL}"><img alt="{Prev_URL}" src="Styles/Simple/Images/Prev.gif" border="0"></a> <!-- END Prev_On -->
              <!-- BEGIN Prev_Off --><img alt="{Prev_URL}" src="Styles/Simple/Images/PrevOff.gif" border="0"><!-- END Prev_Off -->
              <!-- BEGIN Next_On --><a href="{Next_URL}"><img alt="{Next_URL}" src="Styles/Simple/Images/Next.gif" border="0"></a> <!-- END Next_On -->
              <!-- BEGIN Next_Off --><img alt="{Next_URL}" src="Styles/Simple/Images/NextOff.gif" border="0"><!-- END Next_Off --><!-- END Navigator Navigator -->&nbsp;</td>
          </tr>
        </table>
      </td>
    </tr>
  </table>
  <div align="left">
    &nbsp;<a href="#" onclick="window.close();">Cerrar</a> 
  </div>
  <!-- END Grid fotos --><br>
</div>
</body>
</html>